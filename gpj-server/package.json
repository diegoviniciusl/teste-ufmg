{
  "name": "gpj-server",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "node dist/run-app.js",
    "dev": "nodemon src/run-app.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "npx eslint -c .eslintrc.json",
    "migrate": "npx prisma migrate dev",
    "migrate:test": "DATABASE_URL=\"postgresql://user:password@localhost:5432/gr-pericias?schema=public\" npx prisma migrate dev",
    "test": "DATABASE_URL=\"postgresql://user:password@localhost:5432/gr-pericias?schema=public\" jest --runInBand",
    "test:coverage": "DATABASE_URL=\"postgresql://user:password@localhost:5432/gr-pericias?schema=public\" jest --coverage --runInBand"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/json2csv": "^5.0.3",
    "@types/mjml": "^4.7.0",
    "@types/node": "^18.11.9",
    "@types/pdfmake": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@prisma/client": "^4.7.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/supertest": "^2.0.12",
    "prisma": "^4.7.0",
    "bcrypt": "^5.1.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "fastify": "^4.18.0",
    "jest": "^29.5.0",
    "json-schema-to-ts": "^2.6.2",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.0",
    "mjml": "^4.13.0",
    "pdfmake": "^0.2.7",
    "roboto-font": "^0.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0"
  }
}
